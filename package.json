{
  "name": "linkcube-nora",
  "version": "1.1.1",
  "description": "Nodejs R/a/dio Archiver",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "format": "prettier --write \"helpers/**/*.ts\" \"index.ts\" \"cli_start.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json --fix",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A helpers && git add -A index.ts",
    "postversion": "git push && git push --tags",
    "start": "ts-node cli_start.ts -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Linkcube/Nora.git"
  },
  "author": "Linkcube",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Linkcube/Nora/issues"
  },
  "homepage": "https://github.com/Linkcube/Nora#readme",
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.0.20",
    "@ffprobe-installer/ffprobe": "^1.0.12",
    "command-line-args": "^5.1.1",
    "command-line-usage": "^6.1.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "fluent-ffmpeg": "^2.1.2",
    "graphql": "^14.5.8",
    "lodash": "^4.17.15",
    "node-id3": "^0.1.11",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "rimraf": "^3.0.0",
    "sanitize-filename": "^1.6.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/command-line-usage": "^5.0.1",
    "@types/express": "^4.17.2",
    "@types/fluent-ffmpeg": "^2.1.14",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.1.2",
    "@types/request-promise": "^4.1.45",
    "@types/rimraf": "^2.0.3",
    "prettier": "^1.19.1",
    "ts-node": "^8.5.2",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.2"
  },
  "files": [
    "lib/**/*"
  ],
  "bin": "lib/cli_start.js"
}
